generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
  url = env("DATABASE_URL")
}

model TimeEntry {
  id          Int      @id @default(autoincrement())
  date        DateTime
  project     String
  hours       Float
  description String
  createdAt   DateTime @default(now())
}
